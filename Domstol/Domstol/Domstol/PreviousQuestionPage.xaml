<?xml version="1.0" encoding="UTF-8"?>
<ContentPage 
	xmlns="http://xamarin.com/schemas/2014/forms"
	xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
	x:Class="Domstol.PreviousQuestionPage"
	Title="Förgående fråga"

	>
	<ContentPage.Content>

		<Grid>
			<Grid.RowDefinitions>
				<RowDefinition Height="*"/>
				<RowDefinition Height="*"/>
				<RowDefinition Height="auto"/>
			</Grid.RowDefinitions>
			<Grid.ColumnDefinitions>
				<ColumnDefinition Width="*"/>
				<ColumnDefinition Width="auto"/>
				<ColumnDefinition Width="auto"/>
			</Grid.ColumnDefinitions>

			<Frame 
			OutlineColor="Black"
			Grid.Row="0"
			Grid.Column="0"
			Grid.ColumnSpan="3"
			Grid.RowSpan="2"

				>

				<Label
					x:Name="QuestionLabel"
					LineBreakMode="WordWrap"
					/>
				
		
			</Frame>
			<Button 
				Text="Tillbaka"
				Grid.Row="3"
				Grid.Column="0"
				Grid.ColumnSpan="3"
				Clicked="DoneButtonClicked"
			/>
		</Grid>

	</ContentPage.Content>
</ContentPage>
